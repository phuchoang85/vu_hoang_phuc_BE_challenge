{
  "name": "express-api-typescript",
  "author": "p-rk",
  "version": "0.0.2",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "compile": "tsc && cross-env NODE_ENV=development node dist/",
    "dev": "nodemon -e ts --exec \"yarn compile\"",
    "build": "cross-env NODE_ENV=production tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --coverage --detectOpenHandles --forceExit"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "boilerplate",
    "starterkit",
    "express-api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/p-rk/express-api-typescript.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "joi": "^17.2.1",
    "mongoose": "^6.13.8",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.13",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.9.5"
  }
}
